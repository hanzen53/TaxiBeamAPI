<form id="createCallcenterTaskForm">
	<div class="form-group col-lg-12 col-md-12">
		<label for="passTel">เบอร์โทร</label>
		<input id="passTel" name="passTel" type="text" class="form-control" autocomplete="off">
	</div>
	<div class="form-group col-lg-6 col-md-6">
		<label for="fromPos">จุดรับ</label>
		<input id="fromPos" name="fromPos" type="text" class="form-control" autocomplete="off">
	</div>
	<div class="form-group col-lg-6 col-md-6">
		<label for="toPos">จุดส่ง</label>
		<input id="toPos" name="toPos" type="text" class="form-control" autocomplete="off">
	</div>
	<div class="form-group col-lg-12 col-md-12">
		<label for="detail">วิธีการไปรับ</label>
		<textarea id="detail" name="detail" class="form-control" rows="3"></textarea>
	</div>
	<div class="col-lg-12 col-md-12">
		<button type="submit" class="btn btn-block btn-default"><i class="fa fa-floppy-o"></i> บันทึก</button>
	</div>
</form>