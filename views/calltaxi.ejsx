		.js #loader { display: block; position: absolute; left: 100px; top: 0; }
		.se-pre-con {
			position: absolute;
			width: 100%;
			height: 100%;
			z-index: 9999;
			/*background: url(../assets/img/home_screen.png) center center no-repeat #fff;*/
			background: #111;
		}

		.se-pre-click{
			position: fixed;
			left: 0px;
			top: 0px;
			width: 100%;
			height: 100%;
			z-index: 9999;
			background: url(../assets/img/loadingwaiting.gif) center no-repeat rgba(255,255,255,0.6);
			background-size: 300px;
		}
		</style>

		<script src="/libs/jquery/dist/jquery.min.js"></script>	
		
